    "cgx-client-shell": {
      "root": "apps/cgx-client-shell/",
      "sourceRoot": "apps/cgx-client-shell/src",
      "projectType": "application",
      "prefix": "cgx",
      "schematics": {
        "@schematics/angular:component": {
          "inlineStyle": true,
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/cgx-client-shell",
            "index": "apps/cgx-client-shell/src/index.html",
            "main": "apps/cgx-client-shell/src/main.ts",
            "polyfills": "apps/cgx-client-shell/src/polyfills.ts",
            "tsConfig": "apps/cgx-client-shell/tsconfig.app.json",
            "assets": [
              "apps/cgx-client-shell/src/favicon.ico",
              "apps/cgx-client-shell/src/assets"
            ],
            "styles": [
              "apps/cgx-client-shell/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/cgx-client-shell/src/environments/environment.ts",
                  "with": "apps/cgx-client-shell/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "cgx-client-shell:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "cgx-client-shell:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cgx-client-shell:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/cgx-client-shell/src/test.ts",
            "polyfills": "apps/cgx-client-shell/src/polyfills.ts",
            "tsConfig": "apps/cgx-client-shell/tsconfig.spec.json",
            "karmaConfig": "apps/cgx-client-shell/karma.conf.js",
            "styles": [
              "apps/cgx-client-shell/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "apps/cgx-client-shell/src/favicon.ico",
              "apps/cgx-client-shell/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/cgx-client-shell/tsconfig.app.json",
              "apps/cgx-client-shell/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "cgx-client-shell-e2e": {
