{
  "name": "cgx-client",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4100 --open",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:cgx-forms-core": "ng build @cgx-forms/core",
    "build:cgx-forms-dynamic": "ng build @cgx-forms/dynamic",
    "build:cgx-forms": "npm run build:cgx-forms-core && npm run build:cgx-forms-dynamic",
    "build:cgx-ui-controls": "ng build @cgx-ui/controls",
    "build:cgx-ui-layout": "ng build @cgx-ui/layout",
    "build:cgx-ui": "npm run build:cgx-ui-controls && npm run build:cgx-ui-layout",
    "build:cgx-logger": "ng build @cgx-common/logger",
    "build:cgx-config": "ng build @cgx-common/config",
    "build:cgx-common-core": "ng build @cgx-common/core",
    "build:cgx-lookup": "ng build @cgx-common/lookup",
    "build:cgx-common-http":"ng build @cgx-common/http",
    "build:common": "npm run build:cgx-config && npm run build:cgx-logger && npm run build:cgx-lookup && npm run build:cgx-common-core && npm run build:cgx-common-http",
    "build:md-review": "ng build md-review",
    "build:attachment": "ng build attachment",
    "build:audit-trail": "ng build audit-trail",
    "build:all-project-libs": "npm run build:attachment && npm run build:audit-trail && npm run build:md-review",
    "build:all": "npm run build:cgx-forms && npm run build:common && npm run build:cgx-ui && npm run build:all-project-libs",
    "build:run": "npm run build:all && npm run start"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ng-bootstrap/ng-bootstrap": "^4.0.2",
    "@progress/kendo-angular-dateinputs": "^3.5.4",
    "@progress/kendo-angular-dropdowns": "^3.4.2",
    "@progress/kendo-angular-inputs": "^4.0.0",
    "@progress/kendo-angular-intl": "^1.6.1",
    "@progress/kendo-angular-l10n": "^1.3.0",
    "@progress/kendo-theme-default": "^3.2.0",
    "@progress/kendo-theme-material": "^2.2.0",
    "bootstrap": "^4.2.1",
    "core-js": "^2.5.4",
    "jquery": "^3.3.1",
    "rxjs": "~6.3.3",
    "rxjs-compat": "^6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular-devkit/build-ng-packagr": "~0.12.0",
    "@angular/cli": "~7.2.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
